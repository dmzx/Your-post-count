	"type": "phpbb-extension",
	"description": "Shows number of posts counts in quick links.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2015-07-15",
	"keywords": ["phpbb", "extension", "Your post count"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Your post count",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.*@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
